<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <localRepository>/root/.m2/repository</localRepository>
    <mirrors>
        <mirror>
            <id>alimaven</id>
            <name>aliyunmaven</name>
            <url>http://maven.aliyun.com/nexus/content/groups/public</url>
            <mirrorOf>central</mirrorOf>
        </mirror>
    </mirrors>
    <activeProfiles>
        <activeProfile>odianyun</activeProfile>
    </activeProfiles>

    <profiles>
        <profile>
            <id>odianyun</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>

                <repository>
                    <id>odianyun-ThirdPublic</id>
                    <name>ThirdPublic</name>
                    <url>http://maven.ops.oudianyun.com/content/repositories/ThirdPublic/</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>

                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>


                <repository>
                    <id>odianyun-app</id>
                    <name>ThirdPublic</name>
                    <url>http://maven.ops.oudianyun.com/content/repositories/app-architecture-snapshots/</url>
                    <releases>
                        <enabled>false</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>

                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>


                <repository>
                    <id>odianyun-snapshots</id>
                    <name>odianyun snapshots Repository</name>
                    <url>http://maven.ops.oudianyun.com/content/repositories/snapshots-2d9d6-dev/</url>
                    <releases>
                        <enabled>false</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>

                <repository>
                    <id>odianyun-releases</id>
                    <name>odianyun releases Repository</name>
                    <url>http://maven.ops.oudianyun.com/content/repositories/releases/</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>odianyun-public</id>
                    <name>odianyun public Repository</name>
                    <url>http://maven.ops.oudianyun.com/content/repositories/public/</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>

                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>cloudera</id>
                    <url>https://repository.cloudera.com/artifactory/cloudera-repos/</url>
                </repository>
                <!--            <repository>
                                                                        <id>cloudera</id>
                                           <url>https://repository.cloudera.com/content/groups/public</url>
                                </repository> -->

            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>odianyun-public</id>
                    <name>odianyun public Repository</name>
                    <url>http://maven.ops.oudianyun.com/content/repositories/public/</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>
